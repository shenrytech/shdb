    "version": "0.2.0",
    "configurations": [
        {
            "name": "shdbcli",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/shdbcli/main/shdbcli.go",
            "args": [
                "get",
                "shdb.TObject",
                "92d0bac2-bb0f-4a11-843b-4a586986e361"
            ]
        },
        {
            "name": "shdbcli shd<tab><tab>",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/shdbcli/main/shdbcli.go",
            "args": [
                "__complete",
                "get",
                "shd"
            ]
        },
        {
            "name": "shdbcli get nisse 9<tab><tab>",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/shdbcli/main/shdbcli.go",
            "args": [
                "__complete",
                "get",
                "nisse", "9"
            ]
        },
        {
            "name": "shdbcli get nisse 078038e6-a5f4-432a-a129-690689dcc3ca -o \"MyInt: {{.my_int}}\"",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/shdbcli/main/shdbcli.go",
            "args": [
                "get",
                "nisse",
                "078038e6-a5f4-432a-a129-690689dcc3ca",
                "-o", "MyInt: {{.my_int}}"
            ]
        },
        {
            "name": "shdbd",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/shdbd/shdbd.go",
            "args": [
                "-load-test-data"
            ]
        }
    ]
}